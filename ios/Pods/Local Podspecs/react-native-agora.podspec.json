{
  "name": "react-native-agora",
  "version": "4.3.0",
  "summary": "Agora RTC SDK For React Native",
  "homepage": "https://github.com/AgoraIO-Extensions/react-native-agora#readme",
  "license": "MIT",
  "authors": "HUI <luxuhui@agora.io> (https://github.com/LichKing-2234)",
  "platforms": {
    "ios": "12.4"
  },
  "source": {
    "git": "https://github.com/AgoraIO-Extensions/react-native-agora.git",
    "tag": "4.3.0"
  },
  "source_files": "ios/**/*.{h,m,mm}",
  "compiler_flags": "-DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -Wno-comma -Wno-shorten-64-to-32",
  "dependencies": {
    "React-Core": [

    ],
    "RCT-Folly": [
      "2022.05.16.00"
    ],
    "glog": [

    ],
    "AgoraRtcEngine_iOS": [
      "4.3.0"
    ],
    "AgoraIrisRTC_iOS": [
      "4.3.0-build.2"
    ]
  },
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/Headers/Private/Yoga\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20"
  },
  "libraries": "stdc++",
  "frameworks": "ReplayKit"
}
